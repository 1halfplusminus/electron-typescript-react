{
  "name": "electron-mine-typescript",
  "version": "1.0.0",
  "description": "",
  "main": "./src/main.js",
  "scripts": {
    "build-main": "cross-env NODE_ENV=production webpack --config webpack.config.main.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.prod.config.js",
    "start-dev": "cross-env NODE_ENV=development webpack-dev-server --config webpack.dev.config.js",
    "start-electron-dev": "cross-env NODE_ENV=development electron -r ./babel-register.js ./src/main.ts",
    "electron-dev": "concurrently \"yarn start-dev\" \"wait-on http://localhost:2003 && yarn run start-electron-dev \"",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "electron": "cross-env NODE_ENV=production electron -r ./babel-register.js .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "mysuper.app",
    "mac": {
      "category": "your.app.category.type"
    },
    "extends": null,
    "files": [
      "./dist/index.html",
      "./src/main.js",
      "./dist/app.js"
    ]
  },
  "author": "",
  "license": "ISC",
